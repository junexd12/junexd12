local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/junexd12/junexd12/refs/heads/main/Orion%20booting%20library')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Gef Road Script | by Gab tv", HidePremium = false, SaveConfig = true, ConfigFolder = "SB V1", IntroText = "Gef Road !"})

local Tab = Window:MakeTab({
	Name = "Home",
	Icon = "rbxassetid://8834748103",
	PremiumOnly = false
})
Tab:AddLabel("Credit to [ GabTv ]")
Tab:AddLabel("Subcribe to Megaphonk266")
Tab:AddLabel("Wellcome back " ..Player.Name.." ! ") 
Tab:AddButton({
	Name = "Infinite Yield",
	Callback = function()
      		loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Infinite%20Yield%20but%20with%20secure%20dex'))()
  	end    
})
Tab:AddButton({
	Name = "Fly Gui",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/junexd12/junexd12/refs/heads/main/Premium%20Fly%20gui"))()
  	end    
})
Tab:AddButton({
	Name = "Rejoin",
	Callback = function()
      		game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
  	end    
})
Tab:AddButton({
	Name = "Destroy GUI",
	Callback = function()
      		OrionLib:Destroy()
  	end    
})
local Tab2 = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://8834748103",
	PremiumOnly = false
})
Tab2:AddSlider({
	Name = "Tool Reach",
	Min = 2.0,
	Max = 100.0,
	Default = 2.0,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Reach",
	Callback = function(Value)
for _, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        if v:IsA("Tool") then
            local selectionBox = Instance.new("SelectionBox", v.Handle)
            selectionBox.Adornee = v.Handle
            v.Handle.Size = Vector3.new(Value, 2, 1)
            v.GripPos = Vector3.new(0, 0, 0)
        end
    end
	end    
})





local Tab3 = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://8834748103",
	PremiumOnly = false
})
Tab3:AddButton({
	Name = "Revive (Client Sided)",
	Callback = function()
OGL = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Spawn"):FireServer()
task.wait(0.3)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OGL
  	end    
})
Tab3:AddToggle({
	Name = "Spawn Tool (Onspawn Only)",
	Default = false,
	Callback = function(Value)
SpawnTools = Value
while SpawnTools do
game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Spawn"):FireServer()
task.wait()
end
	end    
})
Tab3:AddButton({
	Name = "Full bright",
	Callback = function()
game:GetService("Lighting").Brightness = 2
    game:GetService("Lighting").ClockTime = 12
    game:GetService("Lighting").FogEnd = 786543
    game:GetService("Lighting").GlobalShadows = false
  	end    
})



local Tab4 = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://8834748103",
	PremiumOnly = false
})
Tab4:AddSlider({
	Name = "Fov (Changer)",
	Min = 0,
	Max = 300,
	Default = 80,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "FEILD OF VIEW",
	Callback = function(Value)
game.Workspace.Camera.FieldOfView = Value
FieldOfView = Value
	end    
})

Tab4:AddTextbox({
	Name = "Fov (Changer)",
	Default = "UserFov",
	TextDisappear = false,
	Callback = function(Value)
game.Workspace.Camera.FieldOfView = Value
FieldOfView = Value
	end	  
})
Tab4:AddButton({
	Name = "Reset Fov",
	Callback = function()
game.Workspace.Camera.FieldOfView = 80
FieldOfView = 80
  	end    
})
Tab4:AddSlider({
	Name = "WalkSpeed",
	Min = 20,
	Max = 1000,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end    
})

Tab4:AddTextbox({
	Name = "WalkSpeed",
	Default = "UserSpeed",
	TextDisappear = false,
	Callback = function(Value)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
Walkspeed = Value
	end	  
})
Tab4:AddButton({
	Name = "Reset Walkspeed",
	Callback = function()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 20
Walkspeed = 20
  	end    
})
Tab4:AddToggle({
	Name = "Walkspeed Set Auto",
	Default = false,
	Callback = function(Value)
		KeepWalkspeed = Value
            while KeepWalkspeed do
                if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") ~= nil and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed ~= Walkspeed then
                    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Walkspeed
                end
task.wait()
            end
	end    
})

